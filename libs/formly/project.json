{
	"projectType": "library",
	"sourceRoot": "libs/formly/src",
	"prefix": "fo",
	"targets": {
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/formly"],
			"options": {
				"jestConfig": "libs/formly/jest.config.ts",
				"passWithNoTests": true
