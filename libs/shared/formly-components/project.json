		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": ["libs/shared/formly-components/**/*.ts", "libs/shared/formly-components/**/*.html"]
			}
		}
	},
	"tags": ["shared"]
}
