{
	"projectType": "library",
	"sourceRoot": "libs/shared/formly-components/src",
	"prefix": "ui",
	"targets": {
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/shared/formly-components"],
